{
	"name": "xwp/example-theme",
	"version": "1.0.0",
	"type": "wordpress-theme",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"classmap": [
			"php/",
			"blocks/*/php/"
		]
	},
	"require": {
		"psr/container": "^2.0",
		"ext-libxml": "*",
		"ext-dom": "*"
	}
}
